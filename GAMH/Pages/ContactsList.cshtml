@page
@using Models
@model GAMH.Pages.ContactsListModel
@{
    ViewData["Title"] = "Contacts List";
}

<div class="row container jumbotron">
  
  <h1>Contact List</h1>

  <table class="table table-striped table-hover bg-info">
  
    <thead>
    <tr>
      <th>Date</th>
      <th>From</th>
      <th>Message</th>
      <th>Email</th>
      <th>Phone</th>
    </tr>
    </thead>
   
  
    <tbody>
      @foreach (ContactViewModel contactModel in Model.ContactModelsList.OrderByDescending(contactItem=> contactItem.ContactTime))
      {
        <tr>

        <td>@contactModel.ContactTime</td>
        <td>@contactModel.FirstName</td>
        <td>@contactModel.Message</td>
        <td>@contactModel.Email</td>
        <td>@contactModel.Phone</td>
    
        </tr>

    }
    </tbody>
  </table>


</div>

